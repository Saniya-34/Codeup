{
  "name": "codeup-fullstack",
  "version": "1.0.0",
  "description": "CodeUp - Full Stack Learning Platform",
  "private": true,
  "scripts": {
    "build": "cd client && npm run build",
    "start": "cd api && npm start",
    "dev": "concurrently \"cd api && npm run dev\" \"cd client && npm run dev\"",
    "install-client": "cd client && npm install",
    "install-api": "cd api && npm install",
    "install-all": "npm run install-api && npm run install-client"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hateem72/CodeUp-Deployed"
  },
  "keywords": [
    "education",
    "coding",
    "learning",
    "platform",
    "fullstack"
  ],
  "author": "CodeUp Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}